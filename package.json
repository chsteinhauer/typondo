{
  "name": "@root/monorepo",
  "version": "0.0.0",
  "private": true,
  "root": true,
  "type": "module",
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "apps/*",
    "tooling/*"
  ],
  "scripts": {
    "dev": "cd apps/dimension; npm run dev:app",
    "dev:db": "docker-compose up",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "cache:purge": "rimraf --glob '**/.turbo'; rimraf --glob '**/.cache'; rimraf --glob '**/.eslintcache'; rimraf --glob '**/.stylelintcache'; rimraf --glob '**/tsconfig.tsbuildinfo'; rimraf --glob '**/.next'; rimraf --glob 'libs.*/*/lib'; rimraf --glob 'apps/*/lib'",
    "prepare": "husky || true",
    "postinstall": "patch-package || true"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.8",
    "@changesets/read": "^0.6.1",
    "@changesets/write": "^0.3.2",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@tiptap/pm": "^2.8.0",
    "@tiptap/react": "^2.8.0",
    "@tiptap/starter-kit": "^2.8.0",
    "core-js": "^3.38.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-quill": "^2.0.0",
    "react-quill-new": "^3.3.3",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/node": "^20.17.2",
    "dotenv": "^16.4.5",
    "husky": "^9.1.5",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "stylelint": "^16.9.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
